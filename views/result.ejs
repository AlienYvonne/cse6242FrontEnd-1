<!DOCTYPE html>
<html>
 	<link rel="stylesheet" href="https://unpkg.com/leaflet@1.3.4/dist/leaflet.css"
   integrity="sha512-puBpdR0798OZvTTbP4A8Ix/l+A4dHDD0DGqYW6RQ+9jxkRFclaxxQb/SJAWZfWAkuyeQUytO7+7N4QKrDh+drA=="
   crossorigin=""/>
    <script src="https://unpkg.com/leaflet@1.3.4/dist/leaflet.js"
   integrity="sha512-nMMmRyTVoLYqjP9hrbed9S+FzjZHW5gY1TWCHA5ckwXZBadntCNs8kEqAWdrb9O7rxbCaA4lKTIWjDXZxflOcA=="
   crossorigin=""></script>
   <script src="https://cdnjs.cloudflare.com/ajax/libs/leaflet-ajax/2.1.0/leaflet.ajax.js"></script>
   <script src="http://code.jquery.com/jquery-1.11.0.min.js"></script>

<head>
	<title></title>
</head>
<body>
    <h1>
        This is the title
    </h1>
	 <div id="mapid" style="width: 600px; height: 400px; position: relative;"></div>
	 <script type="text/javascript">
	 	var mymap = L.map('mapid').setView([33.7706675, -84.3661573], 13);
	 	L.tileLayer('https://api.tiles.mapbox.com/v4/{id}/{z}/{x}/{y}.png?access_token=pk.eyJ1IjoibWFwYm94IiwiYSI6ImNpejY4NXVycTA2emYycXBndHRqcmZ3N3gifQ.rJcFIG214AriISLbB6B5aw', {
		maxZoom: 18,
		attribution: 'Map data &copy; <a href="https://www.openstreetmap.org/">OpenStreetMap</a> contributors, ' +
			'<a href="https://creativecommons.org/licenses/by-sa/2.0/">CC-BY-SA</a>, ' +
			'Imagery Â© <a href="https://www.mapbox.com/">Mapbox</a>',
		id: 'mapbox.streets'
	}).addTo(mymap);

  function popUp(feature){
  	// console.log("feature is")
  	// console.log(feature.properties);
  	var coor = feature.geometry.coordinates;
  	// var marker = L.marker(coor).addTo(mymap);
  	// layer.bindPopup(feature.properties.name);
  	var popupDIV = "<b>Hello world!</b>" + feature.properties.address;
  	L.marker(coor).addTo(mymap)
		.bindPopup(popupDIV);
  }
  var geojsonLayer = new L.GeoJSON.AJAX("./curgeo.json", {onEachFeature:popUp });

  geojsonLayer.addTo(mymap);

	 </script>
</body>
</html>